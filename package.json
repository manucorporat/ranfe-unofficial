{
  "name": "renfe-website",
  "version": "0.0.1",
  "description": "Redise√±o de la web de renfe",
  "scripts": {
    "dev-server": "live-server --mount=/:./www --watch=./www",
    "watch": "concurrently \"npm run watch-js\" \"npm run watch-css\" \"npm run dev-server\"",
    "watch-css": "node-sass ./src/index.scss --output ./www/ --watch",
    "watch-js": "rollup -c --watch",
    "build": "npm run build-js && npm run build-css",
    "build-css": "node-sass ./src/index.scss --output ./www/",
    "build-js": "rollup -c",
    "deploy": "bs --directory ./www --no-yarn --no-cleanup --hook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manucorporat/renfe.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manucorporat/renfe/issues"
  },
  "homepage": "https://github.com/manucorporat/renfe#readme",
  "devDependencies": {
    "branchsite": "^4.0.2",
    "concurrently": "^3.5.0",
    "live-server": "^1.2.0",
    "node-sass": "^4.5.3",
    "rollup": "^0.50.0",
    "rollup-plugin-typescript2": "^0.7.0",
    "tslint": "^5.7.0",
    "tslint-ionic-rules": "0.0.11",
    "typescript": "^2.5.3"
  }
}
