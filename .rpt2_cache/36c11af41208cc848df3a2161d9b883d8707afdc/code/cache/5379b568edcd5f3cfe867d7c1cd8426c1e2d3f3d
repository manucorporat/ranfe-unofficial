{"code":"export function toggleVisibility(ev, id) {\r\n    var el = document.getElementById(id);\r\n    if (!el) {\r\n        return;\r\n    }\r\n    if (el.classList.contains('show')) {\r\n        el.classList.remove('show');\r\n    }\r\n    else {\r\n        el.classList.add('show');\r\n        var buttonEle = ev.target;\r\n        var x = buttonEle.offsetLeft;\r\n        var y = buttonEle.offsetTop - el.offsetHeight - 12;\r\n        el.style.left = x + 'px';\r\n        el.style.top = y + 'px';\r\n    }\r\n}\r\nexport function isEmpty(str) {\r\n    return !str || str.length === 0;\r\n}\r\nexport function swapInput(inputId1, inputId2) {\r\n    var input1 = document.getElementById(inputId1);\r\n    var input2 = document.getElementById(inputId2);\r\n    if (input1 && input2) {\r\n        var tmp = input1.value;\r\n        input1.value = input2.value;\r\n        input2.value = tmp;\r\n    }\r\n}\r\nexport function shuffleArray(a) {\r\n    var j, x, i;\r\n    for (i = a.length - 1; i > 0; i--) {\r\n        j = Math.floor(Math.random() * (i + 1));\r\n        x = a[i];\r\n        a[i] = a[j];\r\n        a[j] = x;\r\n    }\r\n}\r\n//# sourceMappingURL=utils.js.map","map":{"version":3,"file":"utils.js","sourceRoot":"","sources":["../src/js/utils.ts"],"names":[],"mappings":"AACA,MAAM,2BAA2B,EAAS,EAAE,EAAU;IACpD,IAAM,EAAE,GAAG,QAAQ,CAAC,cAAc,CAAC,EAAE,CAAC,CAAC;IACvC,EAAE,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;QACR,MAAM,CAAC;IACT,CAAC;IACD,EAAE,CAAC,CAAC,EAAE,CAAC,SAAS,CAAC,QAAQ,CAAC,MAAM,CAAC,CAAC,CAAC,CAAC;QAClC,EAAE,CAAC,SAAS,CAAC,MAAM,CAAC,MAAM,CAAC,CAAC;IAC9B,CAAC;IAAC,IAAI,CAAC,CAAC;QACN,EAAE,CAAC,SAAS,CAAC,GAAG,CAAC,MAAM,CAAC,CAAC;QACzB,IAAM,SAAS,GAAG,EAAE,CAAC,MAAqB,CAAC;QAC3C,IAAM,CAAC,GAAG,SAAS,CAAC,UAAU,CAAC;QAC/B,IAAM,CAAC,GAAG,SAAS,CAAC,SAAS,GAAG,EAAE,CAAC,YAAY,GAAG,EAAE,CAAC;QACrD,EAAE,CAAC,KAAK,CAAC,IAAI,GAAG,CAAC,GAAG,IAAI,CAAC;QACzB,EAAE,CAAC,KAAK,CAAC,GAAG,GAAG,CAAC,GAAG,IAAI,CAAC;IAC1B,CAAC;AACH,CAAC;AAED,MAAM,kBAAkB,GAAkB;IACxC,MAAM,CAAC,CAAC,GAAG,IAAI,GAAG,C