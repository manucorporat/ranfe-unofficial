{"code":"export class HTTPService {\r\n    request(method, url) {\r\n        return new Promise((resolve, reject) => {\r\n            const req = new XMLHttpRequest();\r\n            req.open(method, url);\r\n            req.addEventListener('load', () => {\r\n                resolve(JSON.parse(req.responseText));\r\n            });\r\n            req.addEventListener('error', (ev) => {\r\n                reject(ev);\r\n            });\r\n            req.send();\r\n        });\r\n    }\r\n    GET(url) {\r\n        return this.request('GET', url);\r\n    }\r\n    POST(url) {\r\n        return this.request('GET', url);\r\n    }\r\n}\r\nexport const http = new HTTPService();\r\n//# sourceMappingURL=http.js.map","map":{"version":3,"file":"http.js","sourceRoot":"","sources":["../src/js/http.ts"],"names":[],"mappings":"AACA,MAAM;IAEJ,OAAO,CAAC,MAAc,EAAE,GAAW;QACjC,MAAM,CAAC,IAAI,OAAO,CAAC,CAAC,OAAO,EAAE,MAAM,EAAE,EAAE;YACrC,MAAM,GAAG,GAAG,IAAI,cAAc,EAAE,CAAC;YACjC,GAAG,CAAC,IAAI,CAAC,MAAM,EAAE,GAAG,CAAC,CAAC;YACtB,GAAG,CAAC,gBAAgB,CAAC,MAAM,EAAE,GAAG,EAAE;gBAChC,OAAO,CAAC,IAAI,CAAC,KAAK,CAAC,GAAG,CAAC,YAAY,CAAC,CAAC,CAAC;YACxC,CAAC,CAAC,CAAC;YACH,GAAG,CAAC,gBAAgB,CAAC,OAAO,EAAE,CAAC,EAAE,EAAE,EAAE;gBACnC,MAAM,CAAC,EAAE,CAAC,CAAC;YACb,CAAC,CAAC,CAAC;YACH,GAAG,CAAC,IAAI,EAAE,CAAC;QACb,CAAC,CAAC,CAAC;IACL,CAAC;IAED,GAAG,CAAC,GAAW;QACb,MAAM,CAAC,IAAI,CAAC,OAAO,CAAC,KAAK,EAAE,GAAG,CAAC,CAAC;IAClC,CAAC;IAED,IAAI,CAAC,GAAW;QACd,MAAM,CAAC,IAAI,CAAC,OAAO,CAAC,KAAK,EAAE,GAAG,CAAC,CAAC;IAClC,CAAC;CACF;AAED,MAAM,CAAC,MAAM,IAAI,GAAG,IAAI,WAAW,EAAE,CAAC"}}
